<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="page" namespace="/page" extends="json-default">
		<action name="*" class="com.page.action.PageAction" method="{1}">
			<result name="checkLogin" type="json"></result>
			<result name="adminLogin" type="json"></result>
			<result name="toLogin">/WEB-INF/jsp/login.jsp</result>
			<result name="toRegister">/WEB-INF/jsp/register.jsp</result>
			<result name="toDetail">/WEB-INF/jsp/detail.jsp</result>
			<result name="toRelease">/WEB-INF/jsp/release.jsp</result>
			<result name="toInforPer">/WEB-INF/jsp/personal/information-person.jsp</result>
			<result name="toInforUp">/WEB-INF/jsp/personal/information-update.jsp</result>
			<result name="toInforPas">/WEB-INF/jsp/personal/information-password.jsp</result>
			<result name="toInforPass">/WEB-INF/jsp/personal/information-password-.jsp</result>
			<result name="toAdminLogin">/admin-login.jsp</result>
			<result name="toAdminDetail">/WEB-INF/jsp/admin/admin-detail.jsp</result>
		</action>
	</package>
</struts>